  "config": {
    "forge": {
      "packagerConfig": {
        "name": "Odi timer",
        "overwrite": true,
        "icon": "./src/assets/icons/logo-mac.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": ["win32"]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "additionalDMGOptions": {
              "window": {
                "size": {
                  "width": 600,
                  "height": 400
                }
              }
            },
            "icon": "./src/assets/icons/logo-4x.png",
            "name": "Odi timer",
            "format": "ULFO",
            "overwrite": true
          }
        }
      ]
    }
